# Indexes
indexes:
- - :shard
  - :unique
  - zone
  - zHash

# Attributes
attributes:
  v1:
  - name: zHash
    description: Hash of the object used to shard the data.
    type: integer
    stored: true
    read_only: true
    autogenerated: true
    getter: true
    setter: true

  - name: zone
    description: Sharding zone.
    type: integer
    stored: true
    read_only: true
    autogenerated: true
    getter: true
    setter: true
    transient: true
